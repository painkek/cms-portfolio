{
  "kind": "collectionType",
  "collectionName": "portfolio_items",
  "info": {
    "singularName": "portfolio-item",
    "pluralName": "portfolio-items",
    "displayName": "Portfolio Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "blocks"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "graphics design",
        "video editing",
        "it solutions",
        "social media",
        "event coverage"
      ]
    },
    "client": {
      "type": "string"
    }
  }
}
